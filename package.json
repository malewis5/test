{
  "name": "peakactivity-web-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env IS_DEV=FALSE next start -p 8080",
    "start:dev": "(cross-env PORT=3020 IS_DEV=TRUE next dev -p 3020)",
    "start:dev:cache": "cross-env CACHE_ENDPOINT=http://localhost:8080/cache UNBXD_CACHE_ENDPOINT=http://localhost:8080/cache npm run start:dev",
    "build:dev": "cross-env IS_DEV=TRUE next build",
    "build": "cross-env IS_DEV=FALSE next build",
    "watch:scss": "node-sass --watch src/styles --output-style compressed -o public/styles",
    "analyze": "cross-env IS_DEV=FALSE ANALYZE=true next build",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "type-check": "tsc --noEmit",
    "lint:check": "eslint pages/**/*.{tsx,ts} src/**/*.{tsx,ts} --ignore-pattern 'src/components/Omnimerse/cms/Frontend/*'",
    "lint:fix": "eslint pages/**/*.{tsx,ts} src/**/*.{tsx,ts} --fix --ignore-pattern 'src/components/Omnimerse/cms/Frontend/*'",
    "pretty:check": "prettier  --config .prettierrc.js  --check '{pages,src}/**/*.{ts,tsx}'",
    "pretty:fix": "prettier  --config .prettierrc.js  --write '{pages,src}/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:check && npm run pretty:check && npm run build"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aaronhayes/react-use-hubspot-form": "^2.1.0",
    "@sentry/apm": "^5.21.1",
    "@sentry/browser": "^6.17.9",
    "@sentry/node": "^6.17.9",
    "@sentry/webpack-plugin": "^1.18.7",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "cookies": "^0.8.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.2",
    "fastify-rate-limit": "^5.7.2",
    "formik": "^1.5.8",
    "js-cookie": "^2.2.1",
    "next": "^11.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@types/cookies": "^0.7.7",
    "@types/jest": "^26.0.23",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "@next/eslint-plugin-next": "^11.1.4",
    "eslint-config-next": "^11.1.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.11",
    "nextjs-export-path-map": "^0.1.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.5.5",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "**/*.{html,css,md,json,yaml,graphql}": "prettier --write"
  }
}
